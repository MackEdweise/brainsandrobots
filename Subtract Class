Class Subtract:
  
  def __init__(x,y):
  
    self.value=x-y
    z=abs(y-9) #9's compliment of y
    self.message="Subtract {} from {} by latching {}, adding {} to {}. Result is {}".format(y,x,z,z,x,x-y))
    self.pcode=[1,1,1,1,1,1,1,1]
    self.pcode[z]=0
    self.cncfile="subtractgrbl"
    
    c=Command(self.pcode,self.message)
    c.interpret(2)
    
    return
