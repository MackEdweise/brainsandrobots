Class Multiply:

  def __init__(x,y):
  
    self.value=x/y
    self.pcode=[]
    self.message=""
    self.cncfile="dividegrbl"
    
    s=y
    r=0
    while s<x:
      a=Add(s,y)
      c=Command(a.pcode,a.message)
      c.interpret(2)
      self.pcode.append(a.pcode)
      self.message=self.message+a.message
      
      b=Compare(s,x)
      d=Command(b.pcode,b.message)
      d.interpret(2)
      self.pcode.append(b.pcode)
      self.message=self.message+b.message
      r+=1
      
      p=Add(r,0)      #To display the count r
      e=Command(p.pcode,"Add one to count, yielding {}".format(r))
      e.interpret(2)
    
    
      s=a.value
    
    return
